{
  "analyticConfiguration" : {
    "tenantId" : null,
    "type" : "COMPUTED_METRIC",
    "id" : null,
    "packageId" : null,
    "name" : "Ruby",
    "scope" : {
      "elementName" : null,
      "elementType" : "Ruby",
      "elementTags" : null
    },
    "metrics" : [ {
      "match" : "netuitive.action_controller.action.busytimerequestspersec",
      "properties" : {
        "expression" : "data['action_controller.${1}.${2}.request.total_duration'].actual == 0 ? 0 : (data['action_controller.${1}.${2}.total_requests'].actual / (data['action_controller.${1}.${2}.request.total_duration'].actual / 1000))",
        "for" : "action_controller\\.(.*)\\.(.*)\\.request.total_duration",
        "fqn" : "netuitive.action_controller.${1}.${2}.busytimerequestspersec"
      }
    }, {
      "match" : "netuitive.action_controller.action.requestspersec",
      "properties" : {
        "expression" : "(data['action_controller.${1}.${2}.total_requests'].actual / 300)",
        "for" : "action_controller\\.(.*)\\.(.*)\\.total_requests",
        "fqn" : "netuitive.action_controller.${1}.${2}.requestspersec"
      }
    } ]
  }
}